{
  "openapi": "3.0.0",
  "info": {
    "title": "Flask API",
    "version": "1.0.0",
    "description": "API documentation for the Flask application"
  },
  "paths": {
    "/dns/all": {
      "get": {
        "summary": "Get all DNS records",
        "responses": {
          "200": {
            "description": "Successful response with DNS records",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object"
                  }
                }
              }
            }
          },
          "404": {
            "description": "No DNS records found"
          }
        }
      }
    },
    "/dns/search/{produto}": {
      "get": {
        "summary": "Search DNS record by product name",
        "parameters": [
          {
            "name": "produto",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with DNS record",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean"
                    },
                    "content": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "DNS record not found"
          }
        }
      }
    },
    "/dns/add": {
      "post": {
        "summary": "Add a new DNS record",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "address": {
                    "type": "string"
                  }
                },
                "required": [
                  "name",
                  "address"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "DNS record added successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Failed to add DNS record"
          }
        }
      }
    },
    "/dns/delete": {
      "delete": {
        "summary": "Delete a DNS record",
        "parameters": [
          {
            "name": "id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "DNS record deleted successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Failed to delete DNS record"
          }
        }
      }
    },
    "/dns/names": {
      "get": {
        "summary": "Get all DNS names",
        "responses": {
          "200": {
            "description": "Successful response with DNS names",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean"
                    },
                    "content": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "No DNS names found"
          }
        }
      }
    },
    "/cache/all": {
      "get": {
        "summary": "Get all cache entries",
        "responses": {
          "200": {
            "description": "Successful response with cache entries",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object"
                  }
                }
              }
            }
          },
          "404": {
            "description": "No cache entries found"
          }
        }
      }
    },
    "/cache/fetch/{produto}/{cliente}": {
      "get": {
        "summary": "Fetch cache entry by product and client",
        "parameters": [
          {
            "name": "produto",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "cliente",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with cache entry",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "500": {
            "description": "Error occurred while fetching cache entry"
          }
        }
      }
    },
    "/cache/save": {
      "post": {
        "summary": "Save a cache entry",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cliente": {
                    "type": "string"
                  },
                  "contents": {
                    "type": "object"
                  }
                },
                "required": [
                  "cliente",
                  "contents"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Cache entry saved successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Failed to save cache entry"
          }
        }
      }
    },
    "/cache/delete": {
      "delete": {
        "summary": "Delete a cache entry",
        "parameters": [
          {
            "name": "user_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Cache entry deleted successfully",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Failed to delete cache entry"
          }
        }
      }
    },
    "/client/all": {
      "get": {
        "summary": "Get all clients",
        "responses": {
          "200": {
            "description": "Successful response with clients",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean"
                    },
                    "contents": {
                      "type": "array",
                      "items": {
                        "type": "object"
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "No clients found"
          }
        }
      }
    },
    "/client/add": {
      "post": {
        "summary": "Add a new client",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "user_id": {
                    "type": "string"
                  }
                },
                "required": [
                  "user_id"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Client added successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Failed to add client"
          }
        }
      }
    },
    "/client/delete": {
      "delete": {
        "summary": "Delete a client",
        "parameters": [
          {
            "name": "user_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Client deleted successfully",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Failed to delete client"
          }
        }
      }
    },
    "/client/update": {
      "put": {
        "summary": "Update a client",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "user_id": {
                    "type": "string"
                  },
                  "contents": {
                    "type": "object"
                  }
                },
                "required": [
                  "user_id",
                  "contents"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Client updated successfully",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Failed to update client"
          }
        }
      }
    },
    "/client/search": {
      "get": {
        "summary": "Search for a client by user ID",
        "parameters": [
          {
            "name": "user_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response with client data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ok": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Client not found"
          }
        }
      }
    },
    "/api/ping/{server}": {
      "get": {
        "summary": "Ping a server",
        "parameters": [
          {
            "name": "server",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful ping response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "server": {
                      "type": "string"
                    },
                    "time": {
                      "type": "number"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error occurred during ping"
          }
        }
      }
    },
    "/api/usage": {
      "get": {
        "summary": "Get system usage statistics",
        "responses": {
          "200": {
            "description": "Successful response with system usage",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "boolean"
                    },
                    "ram": {
                      "type": "number"
                    },
                    "storage": {
                      "type": "number"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}